context Rating {

    structure RatingId <valueObject> {
        string value // UUID
    }

    structure Score <valueObject> {
        double value
    }

    structure RatingProvider <entity> {
        RatingId id <identifier>,
        string providerId,
        string userId,
        Score score,
        string comment,
        string timestamp
    }

    structure RatingVehicle <entity> {
        RatingId id <identifier>,
        string vehicleId, // UUID als String
        string userId,    // UUID als String
        Score score,
        string comment,
        string timestamp
    }
}
